{
  "name": "@gracile/cli",
  "version": "0.0.0",
  "description": "A thin, full-stack, web framework",
  "keywords": [
    "full-stack",
    "meta-framework",
    "custom-elements",
    "ssr",
    "server",
    "server-side-rendering",
    "static",
    "web-components",
    "lit"
  ],
  "homepage": "https://gracile.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/gracile-web/gracile",
    "directory": "packages/cli"
  },
  "license": "ISC",
  "author": {
    "name": "Julian Cataldo",
    "email": "contact@juliancataldo.com",
    "url": "https://www.juliancataldo.com"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./ambient": {
      "types": "./ambient.d.ts"
    },
    "./tsconfigs/*": "./tsconfigs/*.json",
    "./tsconfigs/*.json": "./tsconfigs/*"
  },
  "files": [
    "./ambient.d.ts",
    "./dist/**/*",
    "./tsconfigs/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@gracile/engine": "workspace:^",
    "@gracile/internal-utils": "workspace:^",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@gracile/internal-tsconfigs": "workspace:^",
    "@types/node": "^20.11.25",
    "typescript": "5.5.0-beta"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "// @gracile/add": "workspace:^"
}
