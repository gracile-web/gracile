{
  "name": "@gracile/internal-utils",
  "version": "0.3.1-next.0",
  "description": "A thin, full-stack, web framework",
  "keywords": [
    "custom-elements",
    "full-stack",
    "lit",
    "meta-framework",
    "server",
    "server-side-rendering",
    "ssr",
    "static",
    "web-components"
  ],
  "homepage": "https://gracile.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/gracile-web/gracile",
    "directory": "packages/internal/utils"
  },
  "license": "ISC",
  "author": {
    "name": "Julian Cataldo",
    "email": "contact@juliancataldo.com",
    "url": "https://www.juliancataldo.com"
  },
  "type": "module",
  "exports": {
    "./*": "./dist/*.js",
    "./logger": {
      "development": "./dist/logger/logger.dev.js",
      "production": "./dist/logger/logger.build.js",
      "test": "./dist/logger/logger.dev.js",
      "preview": "./dist/logger/logger.dev.js",
      "default": "./dist/logger/logger.build.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "browser": {
        "development": "./dist/env/dev-browser.js",
        "preview": "./dist/env/preview-browser.js",
        "test": "./dist/env/test-browser.js",
        "default": "./dist/env/prod-browser.js"
      },
      "default": {
        "development": "./dist/env/dev-ssr.js",
        "preview": "./dist/env/preview-ssr.js",
        "test": "./dist/env/test-ssr.js",
        "default": "./dist/env/prod-ssr.js"
      }
    }
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@gracile/internal-tsconfigs": "workspace:^",
    "@types/node": "^20.14.8",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "vite": "^5.3.5"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
